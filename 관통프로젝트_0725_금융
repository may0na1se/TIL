nge(len(code_list)):\n","        option_list = []\n","        for _ in option_data:\n","            if _['금융상품코드'] == code_list[i]:\n","                temp_dict = {}\n","                temp_dict.update(\n","                        {\n","                        '저축 금리': _['저축 금리'], '저축 기간': _['저축 기간'], '저축금리유형': _['저축금리유형'],\n","                            '저축금리유형명' : _['저축금리유형명'], '최고 우대금리': _['최고 우대금리']}\n","                            )     #옵션리스트만 담은 임시 딕셔너리를 새로 만들어야 했음\n","                option_list.append(temp_dict) #하나의 리스트에 해당 상품코드에 대한 옵션정보 딕셔너리들을 담을 것\n","\n","        product_option_info = {}\n","        product_option_info.update({'금리정보' : option_list, '금융상품명' : product_data[i]['fin_prdt_nm'],\n","                '금융회사명': product_data[i]['kor_co_nm']})#위에서 만든 옵션정보리스트 + 상품명 + 회사명으로 정리된 딕셔너리\n","        result.append(product_option_info)  #를 결과물 리스트에 추가함. 즉 결과 리스트 하나하나가 검색결과 상품정보 하나하나가 되는 것\n","    return result\n","\n","process_data()\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"cfhgGovsjXn1","executionInfo":{"status":"ok","timestamp":1753428216540,"user_tz":-540,"elapsed":4246,"user":{"displayName":"fidget spinner","userId":"07845832170083969267"}},"outputId":"d3095563-3287-46f1-9f3b-253f132f9e4c"},"execution_count":235,"outputs":[{"output_type":"execute_result","data":{"text/plain":["[{'금리정보': [{'저축 금리': 2.5,\n","    '저축 기간': '1',\n","    '저축금리유형': 'S',\n","    '저축금리유형명': '단리',\n","    '최고 우대금리': 2.5},\n","   {'저축 금리': 2.5,\n","    '저축 기간': '3',\n","    '저축금리유형': 'S',\n","    '저축금리유형명': '단리',\n","    '최고 우대금리': 2.5},\n","   {'저축 금리': 2.5,\n","    '저축 기간': '6',\n","    '저축금리유형': 'S',\n","    '저축금리유형명': '단리',\n","    '최고 우대금리': 2.5},\n","   {'저축 금리': 2.5,\n","    '저축 기간': '12',\n","    '저축금리유형': 'S',\n","    '저축금리유형명': '단리',\n","    '최고 우대금리': 2.5},\n","   {'저축 금리': 2.45,\n","    '저축 기간': '24',\n","    '저축금리유형': 'S',\n","    '저축금리유형명': '단리',\n","    '최고 우대금리': 2.45},\n","   {'저축 금리': 2.45,\n","    '저축 기간': '36',\n","    '저축금리유형': '